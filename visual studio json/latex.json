	"acronym" : {
		"prefix": "ac",
		"body": "\\ac{$1}"
	},
	"table" : 
	{
		"prefix": "tab",
		"body": [
			"\\begin{table}[H]",
			"\t\\caption{$1}",
			"\t\\label{tab:$2}",
			"\t\\centering",
			"\t\\setstretch{1}%minimal text spacing", 
			"\t\\begin{tabular}{|>{\\bfseries}M{4cm}|M{6cm}||M{6cm}|} ",
			"\t\t",
			"\t\\end{tabular}",
			"\\end{table}",
			],
		"description":"tabular"
	}
}

