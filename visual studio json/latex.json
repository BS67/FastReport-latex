{
	// Place your snippets for latex here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	"Crée le document principal": {
		"prefix": "main",
	 	"body": [
			"% LTeX: language=en",
			"%\\b(?!\\\\gls\\{)OS\\b(?!\\})",
			"\\documentclass[12pt,twoside]{report}",
			"\\usepackage[language=en,spacing = 2,link, quote, geometry=1]{main}",
			"\\usepackage[titlenum=2,fancytitle=3,fancypage=4]{styles}",
			"\\usepackage[actif=false]{adddoc}",
			"%\\input{src_tex/acronyms.tex}",
			"\\begin{document}",
			"%\\input{src_tex/pdg.tex}",
			"\\startpreface",
			"%\\input{src_tex/preface.tex} % use prechapter",
			"\\startbody[true][no][true] ",
			"%\\input{src_tex/file.tex}",
			"Test texte",
			"\\tables %tableend if short option for startbody",
			"%\\appendix",
			"\\end{document}",
	 	],
	 	"description": "fichier principal"
	},

	"insertion d'une image": {
		"prefix": "figure",
	 	"body": [
			"\\begin{figure}[tbp]",
    		"\t\\centering",
    		"\t\\includegraphics[width=0.5\\textwidth]{pictures/$2}",
    		"\t\\caption{}",
			"\t\\label{fig:}",
			"\\end{figure}"
	 	],
	 	"description": "insertion d'une image"
	}
	,

	"reconnaissance de la langue Ltex": {
		"prefix": "ltex",
		"body": [
			"% LTeX: language=fr",
			"$2"
		],
		"description": "comment pour ltex"
	},

	"deux images côte à le côte": {
		"prefix": "twopic",
		"body": [
		"\\begin{figure}[]",
    	"\t\\centering",
    	"\t\\twopic{\\includegraphics[width=\\textwidth]{}\\subcaption{}}{\\includegraphics[width=\\textwidth]{}\\subcaption{}}",
    	"\t\\caption{}",
    	"\t\\label{fig:}",
		"\\end{figure}"
		],
		"description": "double picture"
	},
	"sous-numérotation": {
		"prefix": "sb-enumerate",
		"body": [
		"\\begin{enumerate}[sblist]",
    	"\t\\item",
		"\\end{enumerate}"
		],
		"description": "faire une sous numérotation"
	},
	"itemize": {
		"prefix": "itemize",
		"body": [
		"\\begin{itemize}[label=$\\cdot$]",
    	"\t\\item",
		"\\end{itemize}"
		],
		"description": "faire une sous numérotation"
	},

	"légende à coté": {
		"prefix": "sidecap",
		"body": [
		"\\sidecap{\\includegraphics[width=\\textwidth]{}\\caption{}}{}",
		],
		"description": "double picture"
	},
	"italique": {
		"prefix": "it",
		"body": [
		"\\textit{$1}",
		],
		"description": "italique"
	},
	"align": {
		"prefix": "align",
		"body": [
		"\\begin{align}",
		"\t$1",
		"\\end{align}",
		],
		"description": "équation"
	},
	"unité": {
		"prefix": "unit",
		"body": [
		"\\SI{$1}{}",
		],
		"description": "unité"
	},
	"résistance": {
		"prefix": "res",
		"body": [
		"\\SI{$1}{\\kohm}",
		],
		"description": "unité"
	},
	"fréquence": {
		"prefix": "hertz",
		"body": [
		"\\SI{$1}{\\kHz}",
		],
		"description": "unité"
	},
	"accolade": {
		"prefix": "dcases",
		"body": [
		"\\begin{dcases}",
		"$1",
		"\\end{dcases}",
		],
		"description": "cas"
	},

	"première page de garde": {
		"prefix": "pdg1",
		"body": [
			"\\loadgeometry{title}",
			"\\begin{titlepage}",
			"\t\\strut \\\\ \\vspace{2cm}",
			"\t\\begin{flushright}",
				"\t\t\\includegraphics[width=0.50\\textwidth]{pictures/Insas_logo.png}",
			"\t\\end{flushright}",
		"\\vspace{3cm}",
			"\t\\begin{sffamily}",
				"\t\t\\begin{center}",
					"\t\t\t\\\textsc{\\huge title 1 }\\vspace{2cm}",
					"\t\t\t\\vspace{1.5cm}",
		"\\hrule \\vspace{1.3cm}",
						"\t\t\t\t{ \\centering \\huge \\bfseries title 2\\\\ [0.2cm] {\\origdate \\daterange{début}{fin}} \\vspace{1.5cm} }",
		"\\hrule \\vspace{1cm}",
					"\t\t\t\\begin{minipage}{0.5\\textwidth}",
						"\t\t\t\t\\begin{centering}",
						"\t\t\t\t{\\Large \\textbf{Benjamin \\\textsc{Stambach}}}\\vp",
						"\t\t\t\t{\\large \\textsl{MIQ4}}\\vp",
						"\t\t\t\t\\end{centering}",
					"\t\t\t\\end{minipage}%",
					"\t\t\t\\vspace{0.5cm}",
					"\t\t\t\\begin{minipage}{\\textwidth}",
						"\t\t\t\t\\centering",
						"\t\t\t\t\\includegraphics[width=0.5\\textwidth]{pictures/}",
					"\t\t\t\\end{minipage}",
		"\\vspace{2cm}",
		"\\vfill",
		"% Bottom of the page",
				"\t\t\\end{center}",
			"\t\\end{sffamily}",
		"\\end{titlepage}",
		"\\loadgeometry{normal}"
		],
	},
