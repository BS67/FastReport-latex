{
	// Place your snippets for latex here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	"Crée le document principal": {
		"prefix": "main",
	 	"body": [
			"% LTeX: language=fr",
			"\\documentclass[12pt,twoside]{report}",
			"\\usepackage[language=fr,link, quote, floatbarrier, geometry=1]{main}",
			"\\usepackage[titlenum=2,fancytitle=3,fancypage=4, name={Benjamin Stambach \\\\ MIQ3},logo={pictures/Insas_logo.png} ]{styles}",
			"\\usepackage[actif=false]{adddoc}",
			"\\begin{document}",
			"\\input{src_tex/pdg.tex}",
			"\\pagestyle{debut}",
			"\\newpage",
			"\\newgeometry{tmargin=3cm,bmargin=3cm,lmargin=4cm,rmargin=4cm,headheight=100pt, headsep= 1cm, footskip=2cm}",
			"\\singlespacing"
			"\\input{src_tex/resume_remerciement.tex}",
			"\\myspacing",
			"\\loadgeometry{normal}",
			"\\tableofcontents",
			"\\justify",
			"\\sloppy",
			"\\newpage",
			"\\pagestyle{main}",
			"\\input{src_tex/.tex}",
			"\\tables",
			"\\end{document}",
	 		"$2"
	 	],
	 	"description": "fichier principal"
	},

	"insertion d'une image": {
		"prefix": "figure",
	 	"body": [
			"\\begin{figure}[tbp]",
    		"\t\\centering",
    		"\t\\includegraphics[width=0.5\\textwidth]{pictures/$2}",
    		"\t\\caption{}",
			"\t\\label{fig:}",
			"\\end{figure}"
	 	],
	 	"description": "insertion d'une image"
	}
	,

	"reconnaissance de la langue Ltex": {
		"prefix": "ltex",
		"body": [
			"% LTeX: language=fr",
			"$2"
		],
		"description": "comment pour ltex"
	},

	"deux images côte à le côte": {
		"prefix": "twopic",
		"body": [
		"\\begin{figure}[tbp]",
    	"\t\\centering",
    	"\t\\twopic{\\includegraphics[width=\\textwidth]{}\\subcaption{}}{\\includegraphics[width=\\textwidth]{}\\subcaption{}}",
    	"\t\\caption{}",
    	"\t\\label{fig:}",
		"\\end{figure}"
		],
		"description": "double picture"
	},
	"sous-numérotation": {
		"prefix": "sb-enumerate",
		"body": [
		"\\begin{enumerate}[sblist]",
    	"\t\\item",
		"\\end{enumerate}"
		],
		"description": "faire une sous numérotation"
	},
	"itemize": {
		"prefix": "itemize",
		"body": [
		"\\begin{itemize}[label=$\\cdot$]",
    	"\t\\item",
		"\\end{itemize}"
		],
		"description": "faire une sous numérotation"
	},

	"légende à coté": {
		"prefix": "sidecap",
		"body": [
		"\\sidecap{\\includegraphics[width=\\textwidth]{}\\caption{}}{}",
		],
		"description": "double picture"
	},
	"italique": {
		"prefix": "it",
		"body": [
		"\\textit{$1}",
		],
		"description": "italique"
	},
	"align": {
		"prefix": "align",
		"body": [
		"\\begin{align}",
		"\t$1",
		"\\end{align}",
		],
		"description": "équation"
	},
	"unité": {
		"prefix": "unit",
		"body": [
		"\\SI{$1}{}",
		],
		"description": "unité"
	},
	"résistance": {
		"prefix": "res",
		"body": [
		"\\SI{$1}{\\kohm}",
		],
		"description": "unité"
	},
	"fréquence": {
		"prefix": "hertz",
		"body": [
		"\\SI{$1}{\\kHz}",
		],
		"description": "unité"
	},
	"accolade": {
		"prefix": "dcases",
		"body": [
		"\\begin{dcases}",
		"$1",
		"\\end{dcases}",
		],
		"description": "cas"
	},

